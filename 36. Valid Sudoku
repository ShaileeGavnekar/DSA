class Solution {
    public boolean isValidSudoku(char[][] board) {
        Set<String> st = new HashSet<>();
        for(int i = 0; i < 9; i++) {
            for(int j = 0; j < 9; j++) {
                if(board[i][j] == '.') continue;
                String row = i + "-" + board[i][j];
                String col = j + "-" + board[i][j];
                String box = (i / 3) + "-" + (j / 3) + "-" + board[i][j];
                if(st.contains("row" + row) || st.contains("col" + col) || st.contains("box" + box)) return false;
                st.add("row" + row);
                st.add("col" + col);
                st.add("box" + box);
            }
        }
        return true;
    }
}
