class Solution {
    public List<String> generateParenthesis(int n) {
        List<String>ans= new ArrayList<>();
        solve(ans,0,0,"",n);
        return ans;
    }
    public void solve(List<String>ans,int left,int right,String s,int n){
        if(s.length()==2*n) {
            ans.add(s);
            return;
        }
        if(left<n){
            solve(ans,left+1,right,s+"(",n);
        }
        if(right<left){
            solve(ans,left,right+1,s+")",n);
        }
    }
}
