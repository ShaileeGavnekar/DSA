class Solution {
    public String sortString(int n){
        char[] arr= String.valueOf(n).toCharArray();
        Arrays.sort(arr);
        return new String(arr);
    }
    public boolean reorderedPowerOf2(int n) {
        String ans=sortString(n);
        for(int i=0;i<31;i++){
            if(sortString(1<<i).equals(ans)) return true;
        }
        return false;
    }
}
