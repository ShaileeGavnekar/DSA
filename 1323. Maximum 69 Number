class Solution {
    public int maximum69Number (int num) {
        StringBuilder temp= new StringBuilder(String.valueOf(num));
        int ans=0;
        boolean flag=false;
        for(int i=0;i<temp.length();i++){
            char ch=temp.charAt(i);
            if(ch=='6' && flag==false){
                ans= ans*10 + '9'-'0';
                flag=true;
            }else{
                ans= ans*10+ ch-'0';
            }
        }
        return ans;
    }
}
